
@{
    ViewData["Title"] = "Details";
}
<h1>Lyric Details: </h1>
<ul>

    @foreach (var item in Model)
    {
        <div class="row">
            <div class="col-md-4">
                <div class="thumbnail ">
                    <h4>Title: @item.Title</h4>
                    <h4>Artist Name: @item.ArtistName</h4>
                </div>
            </div>
        </div>
    }
</ul>
